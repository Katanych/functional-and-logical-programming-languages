(defun fib1(n)
; Функция рекурентного рассчета F(n)
; последовательности Фибаначчи
    (if (<= n 1)
        1
        (+ (fib1 (- n 1)) 
            (fib1 (- n 2)))
    )
)

(defun fib2(n f1 f2 i)
; Оптимизированный способ рассчета
; F(n) последовательнсоти Фибаначчи
    (if (= n i)
        f1
        (fib2 n (+ f1 f2) f1 (+ i 1))
    )
)

(print (fib2 6 1 1 1))
(print (fib1 6))
